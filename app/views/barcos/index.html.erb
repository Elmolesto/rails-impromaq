<p id="notice"><%= notice %></p>

<h1>Barcos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Nave</th>
      <th>Barandas</th>
      <th>Colectores</th>
      <th>Vtolva1</th>
      <th>Vtolva2</th>
      <th>Al1</th>
      <th>Al2</th>
      <th>Compresorm</th>
      <th>Cabezalm</th>
      <th>Cabezald</th>
      <th>Polines</th>
      <th>Redaire</th>
      <th>Motoresbc03</th>
      <th>Techotolva</th>
      <th>Greating</th>
      <th>Alumbrado</th>
      <th>Caseta</th>
      <th>Ccmmuelle</th>
      <th>C18</th>
      <th>C19</th>
      <th>Numero</th>
      <th>Observaciones</th>
      <th>Empresa</th>
      <th>Varios</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @barcos.each do |barco| %>
      <tr>
        <td><%= barco.nombre %></td>
        <td><%= barco.nave %></td>
        <td><%= barco.barandas %></td>
        <td><%= barco.colectores %></td>
        <td><%= barco.vtolva1 %></td>
        <td><%= barco.vtolva2 %></td>
        <td><%= barco.al1 %></td>
        <td><%= barco.al2 %></td>
        <td><%= barco.compresorm %></td>
        <td><%= barco.cabezalm %></td>
        <td><%= barco.cabezald %></td>
        <td><%= barco.polines %></td>
        <td><%= barco.redaire %></td>
        <td><%= barco.motoresbc03 %></td>
        <td><%= barco.techotolva %></td>
        <td><%= barco.greating %></td>
        <td><%= barco.alumbrado %></td>
        <td><%= barco.caseta %></td>
        <td><%= barco.ccmmuelle %></td>
        <td><%= barco.c18 %></td>
        <td><%= barco.c19 %></td>
        <td><%= barco.numero %></td>
        <td><%= barco.observaciones %></td>
        <td><%= barco.empresa %></td>
        <td><%= barco.varios %></td>
        <td><%= link_to 'Show', barco %></td>
        <td><%= link_to 'Edit', edit_barco_path(barco) %></td>
        <td><%= link_to 'Destroy', barco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Barco', new_barco_path %>
